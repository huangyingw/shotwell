[remote "origin"]
    url = git@github.com:GNOME/shotwell.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[remote "github"]
    url = git@github.com:huangyingw/shotwell.git
    fetch = +refs/heads/*:refs/remotes/github/*
[push]
    remote = github
[gsync]
    remote = origin
    branch = master
[merge]
    allow = true
[branch "dev"]
    remote = github
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[commit]
    message = n
[vrun]
    silent = false
    async = false
[deploy]
    host = ubuntu
    path = ~/myproject/git/linux/GNOME/shotwell
[gdif]
    ignore = .bashrc
    ignore = .gitignore
    ignore = Dockerfile
    ignore = build.rsh
    ignore = docker-compose.yml
    ignore = entrypoint.sh
    ignore = etc/ssh/ssh_config
    ignore = sources.list
